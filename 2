name=('Олег')
surname=("Иванов")
year=("1885")
city= ("Московская губерния")
email=("ivanov@mail.ru")
telephone=("+36859215")


def my_func(name, surname, year, city, email, telephone):
    return ' '.join([name, surname, year, city, email, telephone])


print(my_func(surname, name, year, city, email,telephone))
